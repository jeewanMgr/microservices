package com.jeewan.magar.roomservices.entity;
import javax.persistence.*;

@Entity
@Table(name="ROOM")
public class Room {
	//GenerationType

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name="ROOM_ID")
	private long id;
	
	@Column(name="NAME")
	private String name;
	
	@Column(name="ROOM_NUMBER")
	private String roomNumber;
	
	@Column(name="BED_INFO")
	private String bedInfo;
	
	
	
}
